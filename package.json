{
    "name": "extensible-markdown",
    "displayName": "Extensible Markdown",
    "description": "Converts Markdown to HTML and saves in files, implements TOC with optional heading auto-numbering, HTML abbreviations, attributes, additional substitutions and can be extended by any of the markdown-it plug-ins",
    "version": "6.1.0",
    "publisher": "sakryukov",
    "author": {
        "name": "Sergey A Kryukov",
        "url": "http://www.SAKryukov.org"
    },
    "license": "MIT",
    "icon": "images/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/SAKryukov/vscode-extensible-markdown.git"
    },
    "engines": {
      
  "vscode": ">=1.40.2"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "markdown"
    ],
    "activationEvents": [
        "onLanguage:markdown",
        "onCommand:extensible.markdown.convertToHtml.all"
    ],
    "main": "./out/main.js",
    "contributes": {
        "markdown.markdownItPlugins": true,
        "commands": [
            {
                "command": "extensible.markdown.convertToHtml",
                "title": "Markdown: Convert to HTML",
                "icon": "images/logo.svg"
            },
            {
                "command": "extensible.markdown.convertToHtml.all",
                "title": "Markdown: Convert to HTML all .md files in Workspace"
            }
        ],
        "keybindings": [
            {
                "command": "extensible.markdown.convertToHtml",
                "key": "alt+Q",
                "when": "editorTextFocus"
            },
            {
                "command": "extensible.markdown.convertToHtml.all",
                "key": "alt+A"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "when": "resourceLangId == markdown",
                    "command": "extensible.markdown.convertToHtml",
                    "group": "navigation"
                }
            ],
            "editor/context": [
                {
                    "when": "editorTextFocus && resourceLangId == markdown",
                    "command": "extensible.markdown.convertToHtml",
                    "group": "markdown"
                },
                {
                    "when": "editorTextFocus",
                    "command": "extensible.markdown.convertToHtml.all",
                    "group": "markdown"
                }
            ],
            "explorer/context": [
                {
                    "command": "extensible.markdown.convertToHtml.all",
                    "group": "markdown"
                }
            ]
        },
        "configuration": {
            "type": "object",
            "title": "Extensible Markdown Converter HTML configuration",
            "properties": {
                "markdown.extensibleMarkdown.convertToHtml.reportSuccess": {
                    "type": "boolean",
                    "default": true
                },
                "markdown.extensibleMarkdown.convertToHtml.showHtmlInBrowser": {
                    "type": "boolean",
                    "default": false
                },
                "markdown.extensibleMarkdown.convertToHtml.embedCss": {
                    "type": "boolean",
                    "default": true
                },
                "markdown.extensibleMarkdown.convertToHtml.outputPath": {
                    "type": "string",
                    "default": ""
                },
                "markdown.extensibleMarkdown.convertToHtml.titleClassName": {
                    "type": "string",
                    "default": "title"
                },
                "markdown.extensibleMarkdown.titleLocatorRegex": {
                    "type": "string",
                    "default": "\\{title\\}"
                },
                "markdown.extensibleMarkdown.titleLocatorDecoratorStyle": {
                    "type": "object",
                    "default": {
                        "cursor": "grab",
                        "fontWeight": "bold",
                        "borderRadius": "5px",
                        "color": "yellow",
                        "backgroundColor": "darkRed"
                    }
                },
                "markdown.extensibleMarkdown.abbreviationRegex": {
                    "type": "string",
                    "default": "\\{(.+?)\\}"
                },
                "markdown.extensibleMarkdown.abbreviationDecoratorRegex": {
                    "type": "string",
                    "default": "\\*\\{(.+?)\\}.+?\\*"
                },
                "markdown.extensibleMarkdown.abbreviationDecoratorStyle": {
                    "type": "object",
                    "default": {
                        "cursor": "zoom-in",
                        "borderRadius": "5px",
                        "border": "thin dashed black",
                        "margin": 10,
                        "color": "darkRed",
                        "backgroundColor": "gold"
                    }
                },
                "markdown.extensibleMarkdown.attributeRegex": {
                    "type": "string",
                    "default": "\\{([a-z]+?)\\=([^\"^\\{^\\}]+?)\\}"
                },
                "markdown.extensibleMarkdown.attributeDecoratorStyle": {
                    "type": "object",
                    "default": {
                        "cursor": "copy",
                        "borderRadius": "5px",
                        "color": "white",
                        "backgroundColor": "darkViolet"
                    }
                },
                "markdown.extensibleMarkdown.cssClassRegex": {
                    "type": "string",
                    "default": "\\{\\.([^\"^\\{^\\}]+?)\\}"
                },
                "markdown.extensibleMarkdown.cssClassDecoratorStyle": {
                    "type": "object",
                    "default": {
                        "cursor": "default",
                        "borderRadius": "5px",
                        "color": "yellow",
                        "backgroundColor": "darkBlue"
                    }
                },
                "markdown.extensibleMarkdown.headingId": {
                    "type": "boolean",
                    "default": true
                },
                "markdown.extensibleMarkdown.headingIdPrefix": {
                    "type": "string",
                    "default": "heading."
                },
                "markdown.extensibleMarkdown.tocItemIndentInEm": {
                    "type": "int",
                    "default": 3
                },
                "markdown.extensibleMarkdown.tocRegex": {
                    "type": "string",
                    "default": "^\\@toc$"
                },
                "markdown.extensibleMarkdown.tocDecoratorStyle": {
                    "type": "object",
                    "default": {
                        "cursor": "context-menu",
                        "fontWeight": "bold",
                        "border": "1px solid darkMagenta",
                        "margin": "3px",
                        "color": "darkMagenta",
                        "backgroundColor": "lightBlue"
                    }
                },
                "markdown.extensibleMarkdown.excludeFromTocRegex": {
                    "type": "string",
                    "default": "\\{no-toc\\}"
                },
                "markdown.extensibleMarkdown.excludeFromTocLocatorDecoratorStyle": {
                    "type": "object",
                    "default": {
                        "cursor": "no-drop",
                        "border": "thin dotted black",
                        "borderRadius": "5px",
                        "color": "black",
                        "backgroundColor": "lightGoldenrodYellow"
                    }
                },
                "markdown.extensibleMarkdown.tocIncludeLevels": {
                    "type": "array",
                    "default": [
                        1,
                        2,
                        3,
                        4,
                        5,
                        6
                    ]
                },
                "markdown.extensibleMarkdown.tocContainerClass": {
                    "type": "string",
                    "default": "toc"
                },
                "markdown.extensibleMarkdown.includeLocatorRegex": {
                    "type": "string",
                    "default": "^\\@include\\(([^\\s]+?)\\)$"
                },
                "markdown.extensibleMarkdown.includeLocatorInvalidRegexMessageFormat": {
                    "type": "string",
                    "default": "!!! invalid Regular Expression of include:  \"%s\" !!!"
                },
                "markdown.extensibleMarkdown.includeLocatorFileReadFailureMessageFormat": {
                    "type": "string",
                    "default": "!!! failed to read file \"%s\" !!!"
                },
                "markdown.extensibleMarkdown.includeLocatorDecoratorStyle": {
                    "type": "object",
                    "default": {
                        "cursor": "copy",
                        "borderRadius": "0px",
                        "padding": "3px",
                        "border": "thin solid black",
                        "color": "black",
                        "backgroundColor": "lightgreen"
                    }
                },
                "markdown.extensibleMarkdown.autoNumberingRegex": {
                    "type": "string",
                    "default": "^\\@numbering\\s*?\\{([\\s\\S]*?)\\}"
                },
                "markdown.extensibleMarkdown.autoNumberingDecoratorStyle": {
                    "type": "object",
                    "default": {
                        "cursor": "default",
                        "color": "darkRed",
                        "backgroundColor": "yellow"
                    }
                },
                "markdown.extensibleMarkdown.options.allowHTML": {
                    "type": "boolean",
                    "default": true,
                    "description": "Allows HTML markup in Markdown"
                },
                "markdown.extensibleMarkdown.options.typographer": {
                    "type": "boolean",
                    "default": true
                },
                "markdown.extensibleMarkdown.options.typographerExtensions": {
                    "type": "boolean",
                    "default": true,
                    "description": "Optionally replaces !==, !=, !||, !~~, <=, >=, !~, -+, -:, ||, ~~, ===, ==, !.<, .<, <|, |>, @left, @right, @up and @down with mathematical operators and arrows"
                },
                "markdown.extensibleMarkdown.options.smartQuotes": {
                    "type": "string",
                    "default": "“”‘’",
                    "description": "Enables markdown-it embedded smart quotes feature"
                },
                "markdown.extensibleMarkdown.options.additionalPlugins": {
                    "type": "object",
                    "default": {
                        "absolutePath": "",
                        "relativePath": "",
                        "plugins": [
                            {
                                "name": "",
                                "enable": true,
                                "options": {},
                                "syntacticDecorators": [
                                    {
                                        "enable": true,
                                        "regexString": "",
                                        "tooltipFormat": "%s",
                                        "style": {}
                                    }
                                ]
                            }
                        ]
                    }
                }
            }
        },
        "configurationDefaults": {
            "[markdown]": {
                "editor.codeLens": true,
                "editor.folding": false,
                "editor.lineNumbers": "off",
                "editor.wordWrap": "on"
            }
        }
    },
    "__metadata": {
        "id": "2274c865-8de3-4905-b3ee-661afa12b4c5",
        "publisherId": "d1ee7a62-4a9b-4f51-94ff-cb9d0dc5a6c9",
        "publisherDisplayName": "Sergey A Kryukov"
    }
}