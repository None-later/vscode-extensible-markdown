{
    "name": "convert-markdown-to-html",
    "displayName": "Extensible Markdown Converter",
    "description": "Converts Markdown to HTML and saves in files, can be extended by any of the markdown-it plug-ins",
    "version": "5.8.1",
    "publisher": "sakryukov",
    "author": {
        "name": "Sergey A Kryukov",
        "url": "http://www.SAKryukov.org"
    },
    "license": "MIT",
    "icon": "images/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/SAKryukov/vscode-markdown-to-html.git"
    },
    "engines": {
        "vscode": ">=1.14.1"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "markdown"
    ],
    "activationEvents": [
        "onLanguage:markdown",
        "onCommand:extensible.markdown.convertToHtml.all"
    ],
    "main": "./out/main.js",
    "contributes": {
        "markdown.markdownItPlugins": true,
        "commands": [
            {
                "command": "extensible.markdown.showPreview",
                "title": "Markdown: Open &Preview",
                "icon": "images/preview.svg"
            },
            {
                "command": "extensible.markdown.showPreviewToSide",
                "title": "Markdown: Open Preview to the &Side"
            },
            {
                "command": "extensible.markdown.convertToHtml",
                "title": "Markdown: Convert to &HTML",
                "icon": "images/logo.svg"
            },
            {
                "command": "extensible.markdown.convertToHtml.all",
                "title": "Markdown: Convert to HTML all .md files in &Workspace"
            }
        ],
        "keybindings": [
            {
                "command": "extensible.markdown.showPreview",
                "key": "ctrl+shift+v",
                "mac": "cmd+shift+v",
                "when": "editorTextFocus"
            },
            {
                "command": "extensible.markdown.showPreviewToSide",
                "key": "ctrl+k v",
                "mac": "cmd+k v",
                "when": "editorTextFocus"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "when": "resourceLangId == markdown",
                    "command": "extensible.markdown.convertToHtml",
                    "group": "navigation"
                },
                {
                    "when": "resourceLangId == markdown",
                    "command": "extensible.markdown.showPreview",
                    "group": "navigation"
                }
            ],
            "editor/context": [
                {
                    "when": "editorTextFocus && resourceLangId == markdown",
                    "command": "extensible.markdown.showPreview",
                    "group": "markdownPreview"
                },
                {
                    "when": "editorTextFocus && resourceLangId == markdown",
                    "command": "extensible.markdown.showPreviewToSide",
                    "group": "markdownPreview"
                },
                {
                    "when": "editorTextFocus && resourceLangId == markdown",
                    "command": "extensible.markdown.convertToHtml",
                    "group": "markdown"
                },
                {
                    "when": "editorTextFocus",
                    "command": "extensible.markdown.convertToHtml.all",
                    "group": "markdown"
                }
            ],
            "explorer/context": [
                {
                    "command": "extensible.markdown.convertToHtml.all",
                    "group": "markdown"
                }
            ]
        },
        "configuration": {
            "type": "object",
            "title": "Markdown convert to HTML configuration",
            "properties": {
                "markdown.extension.convertToHtml.reportSuccess": {
                    "type": "boolean",
                    "default": true
                },
                "markdown.extension.convertToHtml.showHtmlInBrowser": {
                    "type": "boolean",
                    "default": false
                },
                "markdown.extension.convertToHtml.embedCss": {
                    "type": "boolean",
                    "default": false
                },
                "markdown.extension.convertToHtml.outputPath": {
                    "type": "string",
                    "default": ""
                },
                "markdown.extension.convertToHtml.titleLocatorRegex": {
                    "type": "string",
                    "default": "^(.*?)\\[\\]\\(title\\)"
                },
                "markdown.extension.convertToHtml.titleLocatorDecoratorStyle": {
                    "type": "object",
                    "default": {
                        "cursor": "copy",
                        "borderRadius": "5px",
                        "color": "yellow",
                        "backgroundColor": "rgba(0, 0, 60, 0.5)"
                    }
                },
                "markdown.extension.convertToHtml.headingId": {
                    "type": "boolean",
                    "default": true
                },
                "markdown.extension.convertToHtml.headingIdPrefix": {
                    "type": "string",
                    "default": "heading."
                },
                "markdown.extension.convertToHtml.tocRegex": {
                    "type": "string",
                    "default": "^\\[\\]\\(toc\\)"
                },
                "markdown.extension.convertToHtml.excludeFromTocRegex": {
                    "type": "string",
                    "default": "\\[\\]\\(notoc\\)"
                },
                "markdown.extension.convertToHtml.tocDecoratorStyle": {
                    "type": "object",
                    "default": {
                        "cursor": "cell",
                        "border": "thin solid black",
                        "color": "white",
                        "backgroundColor": "rgba(255, 160, 0, 0.6)"
                    }
                },
                "markdown.extension.convertToHtml.excludeFromTocLocatorDecoratorStyle": {
                    "type": "object",
                    "default": {
                        "border": "thin solid black",
                        "color": "white",
                        "backgroundColor": "rgba(0, 0, 0, 0.4)"
                    }
                },
                "markdown.extension.convertToHtml.tocIncludeLevels": {
                    "type": "array",
                    "default": [
                        1,
                        2,
                        3,
                        4,
                        5,
                        6
                    ]
                },
                "markdown.extension.convertToHtml.tocContainerClass": {
                    "type": "string",
                    "default": "toc"
                },
                "markdown.extension.convertToHtml.defaultListElement": {
                    "type": "string",
                    "default": "ul"
                },
                "markdown.extension.convertToHtml.listElements": {
                    "type": "array",
                    "default": [
                        "ul",
                        "ul",
                        "ul",
                        "ul",
                        "ul",
                        "ul"
                    ]
                },
                "markdown.extension.convertToHtml.defaultListElementAttributeSet": {
                    "type": "object",
                    "default": {
                        "style": "list-style-type: none;"
                    }
                },
                "markdown.extension.convertToHtml.listElementAttributeSets": {
                    "type": "array",
                    "default": []
                },
                "markdown.extension.convertToHtml.idPrefix": {
                    "type": "string",
                    "default": "headings."
                },
                "markdown.extension.convertToHtml.includeLocatorRegex": {
                    "type": "string",
                    "default": "\\[\\]\\(include\\(([^\\s]+?)\\)\\)"
                },
                "markdown.extension.convertToHtml.includeLocatorInvalidRegexMessageFormat": {
                    "type": "string",
                    "default": "!!! invalid Regular Expression of include:  \"%s\" !!!"
                },
                "markdown.extension.convertToHtml.includeLocatorFileReadFailureMessageFormat": {
                    "type": "string",
                    "default": "!!! failed to read file \"%s\" !!!"
                },
                "markdown.extension.convertToHtml.includeLocatorDecoratorStyle": {
                    "type": "object",
                    "default": {
                        "margin": "10px",
                        "cursor": "copy",
                        "borderRadius": "5px",
                        "color": "black",
                        "backgroundColor": "rgba(0, 0, 250, 0.2)"
                    }
                },
                "markdown.extension.convertToHtml.autoNumbering": {
                    "type": "object",
                    "default": { "enable": false }
                },
                "markdown.extension.convertToHtml.autoNumberingRegex": {
                    "type": "string",
                    "default": "^\\[\\]\\(\\=numbering([\\s\\S]*?)\\=\\)"
                },
                "markdown.extension.convertToHtml.autoNumberingDecoratorStyle": {
                    "type": "object",
                    "default": {
                        "cursor": "copy",
                        "color": "darkRed",
                        "backgroundColor": "rgba(255, 255, 0, 0.5)"
                    }
                },
                "markdown.extension.convertToHtml.options.allowHTML": {
                    "type": "boolean",
                    "default": true
                },
                "markdown.extension.convertToHtml.options.linkify": {
                    "type": "boolean",
                    "default": false
                },
                "markdown.extension.convertToHtml.options.br": {
                    "type": "boolean",
                    "default": true
                },
                "markdown.extension.convertToHtml.options.typographer": {
                    "type": "boolean",
                    "default": true
                },
                "markdown.extension.convertToHtml.options.smartQuotes": {
                    "type": "string",
                    "default": "“”‘’"
                },
                "markdown.extension.convertToHtml.options.additionalPlugins": {
                    "type": "object",
                    "default": {
                        "absolutePath": "",
                        "relativePath": "",
                        "plugins": [
                            {
                                "name": "",
                                "enable": true,
                                "options": {},
                                "syntacticDecorators": [
                                    {
                                        "enable": true,
                                        "regexString": "",
                                        "tooltipFormat": "%s",
                                        "style": {}
                                    }
                                ]
                            }
                        ]
                    }
                }
            }
        },
        "configurationDefaults": {
            "[markdown]": {
                "editor.codeLens": true,
                "editor.folding": false,
                "editor.lineNumbers": "off",
                "editor.wordWrap": "on"
            }
        }
    },
    "dependencies": {
        "vscode": "^1.14.1"
    },
    "devDependencies": {
        "vscode": "^1.14.1"
    },
    "extensionDependencies": [
        "vscode.markdown"
    ],
    "__metadata": {
        "id": "2274c865-8de3-4905-b3ee-661afa12b4c5",
        "publisherId": "d1ee7a62-4a9b-4f51-94ff-cb9d0dc5a6c9",
        "publisherDisplayName": "Sergey A Kryukov"
    }
}